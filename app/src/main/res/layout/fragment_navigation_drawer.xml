<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/nav_drawer_width"
    android:layout_height="match_parent"
    android:background="@drawable/slide_menu">


    <ImageView
        android:id="@+id/findPeople"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="170dp"
        android:layout_marginLeft="90dp"
        android:background="@drawable/icon_find"/>

    <ImageView
        android:id="@+id/message"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="175dp"
        android:layout_marginLeft="200dp"
        android:background="@drawable/message"/>

    <ImageView
        android:id="@+id/settings"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="240dp"
        android:layout_marginLeft="240dp"
        android:background="@drawable/icon_setting"/>


    <ImageView
        android:id="@+id/notification"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="250dp"
        android:background="@drawable/icon_notification"/>

    <ImageView
        android:id="@+id/manageEvent"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:layout_marginTop="400dp"
        android:layout_marginLeft="160dp"
        android:background="@drawable/icon_manage"/>


    <ImageView
        android:id="@+id/zicket"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:layout_marginTop="360dp"
        android:layout_marginLeft="60dp"
        android:background="@drawable/icon_zicket"/>


    <ImageView
        android:id="@+id/home"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:layout_marginTop="280dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/icon_home"/>

</RelativeLayout>